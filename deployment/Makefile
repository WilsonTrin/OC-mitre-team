	echo '#define COMPRIVATE ""' > com_secrets.h
	echo '#define APPUBLIC "MIGfMA0GCSqGSIb3DQEBAQUAA4GNADCBiQKBgQCrNIqilA4SU6XZfWNo5fZvkJD8aVj7e/eDz+T3RlxX7JwxJcV4r8fm1Vs9Z9GR5sQE9jm3YzVZYBxZaOYWsnK/rPt6MhpQGxxMaabWBBGsVrofFOugPcAHbYGB0RcArihDE+FVczEF8kPPOknhWNPQVV1G4DhS4WrOQHyeaBHAVwIDAQAB"' >> com_secrets.h

