# echo "#define SECRET 1234" > global_secrets.h
# function to check required arguments
check_defined = \
	$(strip $(foreach 1,$1, \
		$(call __check_defined,$1)))
__check_defined = \
	$(if $(value $1),, \
		$(error Undefined $1))

all:
	echo "#define APPRIVATE \"\"" > ap_secrets.h
	echo "#define COMPUBLIC \"\"" >> ap_secrets.h

	echo "#define COMPRIVATE \"\"" > com_secrets.h
	echo "#define APPUBLIC \"\"" >> com_secrets.h
# These are now base64 encoded to preserve newlines...
clean:
	rm -f ap_secrets.h
	rm -f com_secrets.h
